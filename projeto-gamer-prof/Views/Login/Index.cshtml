@model Jogador

@{

    ViewData["Title"] = "Login";

}

<h1>Tela de login</h1>
<h2>
    @TempData.Peek("Message")

</h2>


<form>

    <label for="">Email</label>
    <input type="email" name="Email" required>

    <label for="">Senha</label>
    <input type="password" name="Senha" required>

    <button type="submit">Logar</button>
</form>